{
    "name": "@doist/reactist",
    "description": "Open source React components by Doist",
    "author": "Henning Muszynski <henning@doist.com> (http://doist.com)",
    "version": "9.1.6",
    "license": "MIT",
    "homepage": "https://github.com/Doist/reactist#readme",
    "repository": "git+https://github.com/Doist/reactist.git",
    "prettier": "@doist/prettier-config",
    "main": "lib/index.js",
    "module": "es/index.js",
    "typings": "lib/index.d.ts",
    "unpkg": "dist/reactist.cjs.production.min.js",
    "sideEffects": [
        "**/*.css"
    ],
    "files": [
        "dist",
        "es",
        "lib",
        "styles"
    ],
    "engines": {
        "node": "^14.5.5",
        "npm": "^6.14.11"
    },
    "scripts": {
        "setup": "npm install && npm run validate",
        "validate": "npm run lint && npm run type-check && npm run test",
        "start": "tsdx watch --onSuccess \"./scripts/organize-styles.sh\"",
        "build-all": "npm run build && npm run build:storybook",
        "build": "scripts/build.sh",
        "build:storybook": "build-storybook -o docs",
        "clean": "rimraf es lib styles dist",
        "test": "tsdx test --passWithNoTests",
        "test:watch": "npm run test -- --watch",
        "test:coverage": "tsdx test --coverage",
        "type-check": "tsc --noEmit -p ./tsconfig.json",
        "lint": "tsdx lint src stories",
        "storybook": "start-storybook -p 6006",
        "prettify": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,scss,less,md,mdx}\""
    },
    "peerDependencies": {
        "classnames": "^2.2.5",
        "react": "^15.5.4 || ^16.2.0",
        "react-dom": "^15.5.4 || ^16.2.0",
        "prop-types": "^15.6.2"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/plugin-proposal-class-properties": "^7.10.1",
        "@babel/plugin-proposal-export-default-from": "^7.0.0",
        "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.10.1",
        "@babel/plugin-transform-spread": "^7.0.0",
        "@babel/polyfill": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.10.1",
        "@babel/register": "^7.0.0",
        "@doist/eslint-config": "^6.0.2",
        "@doist/prettier-config": "^3.0.5",
        "@storybook/addon-actions": "^6.2.1",
        "@storybook/addon-docs": "^6.2.1",
        "@storybook/addon-knobs": "^6.2.1",
        "@storybook/addon-links": "^6.2.1",
        "@storybook/addon-options": "^5.3.21",
        "@storybook/addons": "^6.2.1",
        "@storybook/react": "^6.2.1",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.0.4",
        "@testing-library/user-event": "^12.1.7",
        "@types/cheerio": "^0.22.22",
        "@types/classnames": "^2.2.10",
        "@types/enzyme": "^3.10.7",
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/react": "^16.9.52",
        "@types/react-dom": "^16.9.8",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "autoprefixer": "^9.8.0",
        "babel-core": "^7.0.0-bridge.0",
        "babel-loader": "^8.1.0",
        "classnames": "^2.2.5",
        "css-loader": "^4.2.2",
        "cssnano": "^4.1.10",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.5",
        "enzyme-to-json": "^3.6.1",
        "eslint": "^7.25.0",
        "eslint-config-prettier": "^6.12.0",
        "eslint-plugin-jest": "^24.1.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.23.2",
        "husky": "^4.3.0",
        "jest": "^24.7.1",
        "jest-environment-jsdom-sixteen": "^1.0.3",
        "less": "^3.8.1",
        "less-loader": "^7.0.0",
        "lint-staged": "^10.4.0",
        "mini-css-extract-plugin": "^0.11.0",
        "mockdate": "^3.0.2",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "path": "^0.12.7",
        "prettier": "2.1.2",
        "raw-loader": "^4.0.1",
        "react": "^16.13.1",
        "react-docgen-typescript-loader": "^3.7.2",
        "react-dom": "^16.13.1",
        "react-is": "^16.13.1",
        "react-storybook-addon-chapters": "^3.1.7",
        "react-svg-loader": "^3.0.3",
        "react-test-renderer": "^16.13.1",
        "rimraf": "^3.0.2",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-styles": "^3.10.0",
        "style-loader": "^0.23.1",
        "svg-url-loader": "^6.0.0",
        "ts-loader": "^8.0.2",
        "tsdx": "^0.13.2",
        "tslib": "^2.0.0",
        "typescript": "^3.9.2",
        "webpack": "^4.43.0"
    },
    "dependencies": {
        "@storybook/addon-controls": "^6.2.8",
        "@storybook/addon-postcss": "^2.0.0",
        "dayjs": "^1.8.10",
        "react-markdown": "^5.0.3",
        "reakit": "1.3.0"
    }
}
